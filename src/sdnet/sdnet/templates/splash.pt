<html xmlns="http://www.w3.org/1999/xhtml"
      metal:use-macro="master">

<body>
  <div metal:fill-slot="body-content">

    <div tal:replace="structure prolog" />

    <div id="recent">
     <h3>Recent Articles</h3>
     <ul>
      <li tal:repeat="entry entries">
        <a href="${entry['url']}"
        >${entry['title']}</a>
      </li>
     </ul>
    </div>
  </div>
</body>
</html>
